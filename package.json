    "@expo/plist": "^0.0.20"
  },
  "devDependencies": {
    "@types/react": "~18.2.21",
    "@types/react-native": "0.72.2",
    "expo-module-scripts": "^3.1.0",
    "expo-modules-core": "^1.5.11",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.73.6",
